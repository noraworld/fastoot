{
  "name": "fastoot",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Quickly post your status to Mastodon",
  "homepage_url": "https://github.com/noraworld/fastoot",

  "permissions": [
    "storage"
  ],

  "browser_action": {
    "default_title": "fastoot",
    "default_popup": "popup.html"
  },

  "options_page": "options.html"
}
